[
   {
      "type": 1,
      "title":"SYNTROPY agent",
      "description":"Connecting machines",
      "logo": "https://imgur.com/download/eKUVKgE",
      "categories":[
         "Networking",
         "VPN"
      ],
      "platform":"linux",
      "image":"syntropy/agent:latest",
      "name":"syntropy-agent",
      "privileged": true,
      "network":"host",
      "env":[
         {
            "name":"SYNTROPY_API_KEY",
            "label":"SYNTROPY API KEY (mandatory)"
         },
         {
            "name":"SYNTROPY_CONTROLLER_URL",
            "label":"SYNTROPY Controller URL (mandatory)",
            "default": "controller-prod-platform-agents.syntropy.network"
         },
         {
            "name":"SYNTROPY_NETWORK_API",
            "label": "API to use",
            "default":"docker"
         },
         {
            "name":"SYNTROPY_TAGS",
            "label":"Tags (optional)"
         },
         {
            "name":"SYNTROPY_AGENT_NAME",
            "label":"Agent name (optional)"
         }, 
         {
            "name":"SYNTROPY_COUNTRY",
            "label":"Country (optional)"
         }, 
         {
            "name":"SYNTROPY_CITY",
            "label":"City (optional)"
         }, 
         {
            "name":"SYNTROPY_LAT",
            "label":"Latitude (optional)"
         },
         {
            "name":"SYNTROPY_LON",
            "label":"Longitude (optional)"
         }
      ],
      "volumes":[
         {
            "container":"/var/run/docker.sock",
            "bind":"/var/run/docker.sock",
            "readonly":true
         }
      ]
   }
]
